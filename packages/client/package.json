{
  "name": "@webcomponents/internal-site-client",
  "private": true,
  "version": "0.0.0",
  "description": "Client JavaScript used on webcomponents.org",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --pretty",
      "files": [
        "tsconfig.json",
        "../../tsconfig.base.json",
        "src/**/*.ts"
      ],
      "output": [
        "lib/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    }
  },
  "dependencies": {
    "lit": "^2.3.1"
  }
}
