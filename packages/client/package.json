{
  "name": "@webcomponents/internal-site-client",
  "private": true,
  "version": "0.0.0",
  "description": "Client JavaScript used on webcomponents.org",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "tsc": "wireit",
    "rollup": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "tsc",
        "rollup"
      ]
    },
    "tsc": {
      "command": "tsc --pretty",
      "files": [
        "tsconfig.json",
        "../../tsconfig.base.json",
        "src/**/*.ts"
      ],
      "output": [
        "lib/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "rollup": {
      "command": "rollup -c",
      "dependencies": [
        "tsc"
      ],
      "files": [
        "rollup.config.js"
      ],
      "output": [
        "bundled"
      ]
    }
  },
  "dependencies": {
    "lit": "^2.3.1",
    "tslib": "^2.4.0"
  }
}
